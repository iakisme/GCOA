package com.qylm.dxo;

import com.qylm.dto.vehicle.VehiclePeccancyCreateDto;
import com.qylm.entity.VehiclePeccancy;

public final class VehiclePeccancyCreateDxo {

	public static void dtoToEntity(VehiclePeccancyCreateDto dto, VehiclePeccancy entity) {
		entity.setVehicleInfo(dto.getVehicleInfo());
		entity.setEmployee(dto.getEmployee());
		entity.setCause(dto.getCause());
		entity.setAddress(dto.getAddress());
		entity.setCost(dto.getCost());
		entity.setScore(dto.getScore());
		entity.setRemark(dto.getRemark());
		entity.setState(dto.isState());
		entity.setPeccancyDate(dto.getPeccancyDate());
		entity.setCreater(dto.getCreater());
		entity.setBelongingUser(dto.getBelongingUser());
	}

	public static void entityToDto(VehiclePeccancy entity, VehiclePeccancyCreateDto dto) {
		dto.setVehicleInfo(entity.getVehicleInfo());
		dto.setEmployee(entity.getEmployee());
		dto.setCause(entity.getCause());
		dto.setAddress(entity.getAddress());
		dto.setCost(entity.getCost());
		dto.setRemark(entity.getRemark());
		dto.setState(entity.isState());
		dto.setPeccancyDate(entity.getPeccancyDate());
		dto.setScore(entity.getScore());
		dto.setCreater(entity.getCreater());
		dto.setBelongingUser(entity.getBelongingUser());
	}

}
