package com.qylm.dxo;

import com.qylm.dto.fitting.FittingStorageCreateDto;
import com.qylm.entity.FittingStorage;

public final class FittingStorageCreateDxo {

	public static void dtoToEntity(FittingStorageCreateDto dto, FittingStorage entity) {
		entity.setNumber(dto.getNumber());
		entity.setStorageDate(dto.getStorageDate());
		entity.setRemark(dto.getRemark());
		entity.setState(dto.isState());
		entity.setCreater(dto.getCreater());
		entity.setBelongingUser(dto.getBelongingUser());
	}

	public static void entityToDto(FittingStorage entity, FittingStorageCreateDto dto) {
		dto.setNumber(entity.getNumber());
		dto.setStorageDate(entity.getStorageDate());
		dto.setRemark(entity.getRemark());
		dto.setState(entity.isState());
		dto.setCreater(entity.getCreater());
		dto.setBelongingUser(entity.getBelongingUser());
	}

}
