package com.qylm.dxo;

import com.qylm.dto.engineeringMaterials.MaterialsGrantCreateDto;
import com.qylm.entity.MaterialsGrant;

public final class MaterialsGrantCreateDxo {

	public static void dtoToEntity(MaterialsGrantCreateDto dto, MaterialsGrant entity) {
		entity.setEngineeringProjectDetail(dto.getEngineeringProjectDetail());
		entity.setEmployee(dto.getEmployee());
		entity.setReceiveSum(dto.getReceiveSum());
		entity.setState(dto.getState());
		entity.setType(dto.getType());
		entity.setRemark(dto.getRemark());
		entity.setCreater(dto.getCreater());
		entity.setBelongingUser(dto.getBelongingUser());
	}

	public static void entityToDto(MaterialsGrant entity, MaterialsGrantCreateDto dto) {
		dto.setEngineeringProjectDetail(entity.getEngineeringProjectDetail());
		dto.setEmployee(entity.getEmployee());
		dto.setReceiveSum(entity.getReceiveSum());
		dto.setState(entity.getState());
		dto.setType(entity.getType());
		dto.setRemark(entity.getRemark());
		dto.setCreater(entity.getCreater());
		dto.setBelongingUser(entity.getBelongingUser());
	}

}
