package com.qylm.dto.vehicle;

import java.io.Serializable;
import java.util.Date;

/**
 * 保存车辆违章事故管理画面需要的值
 * @author smj
 */
public class VehiclePeccancyManageDto implements Serializable {

	/**
	 * serialVersionUID
	 */
	private static final long serialVersionUID = 30735022651053690L;

	/**
	 * 车辆编号
	 */
	private String number;
	
	/**
	 * 违章或事故原因
	 */
	private String cause;
	
	/**
	 * 违章事故日期（开始）
	 */
	private Date beginPeccancyDate;
	
	/**
	 * 违章事故日期（结束）
	 */
	private Date endPeccancyDate;

	/**
	 * @return the number
	 */
	public String getNumber() {
		return number;
	}

	/**
	 * @param number the number to set
	 */
	public void setNumber(String number) {
		this.number = number;
	}

	/**
	 * @return the cause
	 */
	public String getCause() {
		return cause;
	}

	/**
	 * @param cause the cause to set
	 */
	public void setCause(String cause) {
		this.cause = cause;
	}

	/**
	 * @return the beginPeccancyDate
	 */
	public Date getBeginPeccancyDate() {
		return beginPeccancyDate;
	}

	/**
	 * @param beginPeccancyDate the beginPeccancyDate to set
	 */
	public void setBeginPeccancyDate(Date beginPeccancyDate) {
		this.beginPeccancyDate = beginPeccancyDate;
	}

	/**
	 * @return the endPeccancyDate
	 */
	public Date getEndPeccancyDate() {
		return endPeccancyDate;
	}

	/**
	 * @param endPeccancyDate the endPeccancyDate to set
	 */
	public void setEndPeccancyDate(Date endPeccancyDate) {
		this.endPeccancyDate = endPeccancyDate;
	}

}
