package com.qylm.dto.vehicle;

import java.io.Serializable;
import java.util.Date;

/**
 * 保存车辆维修管理画面需要的值
 * @author smj
 */
public class VehicleServicingManageDto implements Serializable {

	/**
	 * serialVersionUID
	 */
	private static final long serialVersionUID = -8396541323616733969L;

	/**
	 * 车辆编号
	 */
	private String number;
	
	/**
	 * 故障
	 */
	private String breakdown;
	
	/**
	 * 维修日期（开始）
	 */
	private Date beginGervicingDate;
	
	/**
	 * 维修日期（结束）
	 */
	private Date endGervicingDate;

	/**
	 * @return the number
	 */
	public String getNumber() {
		return number;
	}

	/**
	 * @param number the number to set
	 */
	public void setNumber(String number) {
		this.number = number;
	}

	/**
	 * @return the breakdown
	 */
	public String getBreakdown() {
		return breakdown;
	}

	/**
	 * @param breakdown the breakdown to set
	 */
	public void setBreakdown(String breakdown) {
		this.breakdown = breakdown;
	}

	/**
	 * @return the beginGervicingDate
	 */
	public Date getBeginGervicingDate() {
		return beginGervicingDate;
	}

	/**
	 * @param beginGervicingDate the beginGervicingDate to set
	 */
	public void setBeginGervicingDate(Date beginGervicingDate) {
		this.beginGervicingDate = beginGervicingDate;
	}

	/**
	 * @return the endGervicingDate
	 */
	public Date getEndGervicingDate() {
		return endGervicingDate;
	}

	/**
	 * @param endGervicingDate the endGervicingDate to set
	 */
	public void setEndGervicingDate(Date endGervicingDate) {
		this.endGervicingDate = endGervicingDate;
	}

}
